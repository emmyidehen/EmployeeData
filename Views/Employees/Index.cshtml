@model List<EmployeeCRUD2.Models.Domain.Employee>

@{
}

<h1>Employees</h1>
<table class="table">
 <thead>
  <tr>
   <th>Id</th>
   <th>Name</th>
   <th>Email</th>
   <th>Department</th>
   <th>Salary</th>
   <th>Date of Birth</th>
  </tr>
 </thead>
 <tbody>
		@foreach (var employee in Model)
		{
	  <tr>
	   <td>@employee.Id</td>
	   <td>@employee.Name</td>
	   <td>@employee.Email</td>
	   <td>@employee.Department</td>
	   <td>@employee.Salary</td>
	   <td>@employee.DateofBirth.ToString("dd-MM-yyyy")</td>
	   <td><a href="Employee/View/@employee.Id">View</a></td>

	  </tr>
		}
 </tbody>


</table>